48\u0430\u0433 \u0432\u043f\u0435\u0440\u0435\u0434 \u0441\u0432\u043e\u0438\u0445 \u043a\u043e\u043d\u043a\u0443\u0440\u0435\u043d\u0442\u043e\u0432. \u041a\u0440\u0443\u0442\u043e, \u043f\u0440\u0430\u0432\u0434\u0430?\ud83d\udcab\n\n\ud83d\udccc \u0422\u0435\u043f\u0435\u0440\u044c, \u043d\u0430 \u043e\u0447\u0435\u0440\u0435\u0434\u0438 3 \u0443\u0440\u043e\u043a - \u0443\u043f\u0430\u043a\u043e\u0432\u043a\u0430 \u043f\u0440\u043e\u0444\u0438\u043b\u044f.\n\u041f\u043e\u0441\u043b\u0435 \u043d\u0435\u0433\u043e \u0442\u044b \u043d\u0430\u0443\u0447\u0438\u0448\u044c\u0441\u044f \u0441\u043e\u0441\u0442\u0430\u0432\u043b\u044f\u0442\u044c \u0446\u0435\u043f\u043b\u044f\u044e\u0449\u0443\u044e \u0448\u0430\u043f\u043a\u0443 \u043f\u0440\u043e\u0444\u0438\u043b\u044f, \u043f\u043e\u0434\u0431\u0438\u0440\u0430\u0442\u044c \u043f\u0440\u0430\u0432\u0438\u043b\u044c\u043d\u044b\u0435 \u0442\u0435\u0437\u0438\u0441\u044b \u0438 \u043f\u043e\u0439\u043c\u0435\u0448\u044c, \u0447\u0442\u043e, \u043e\u0431\u044f\u0437\u0430\u0442\u0435\u043b\u044c\u043d\u043e, \u0434\u043e\u043b\u0436\u043d\u043e \u0431\u044b\u0442\u044c \u0432 \u0430\u043a\u043a\u0430\u0443\u043d\u0442\u0435.\n\n\u2705 \u0421\u0441\u044b\u043b\u043a\u0430 \u043d\u0430 \u0443\u0440\u043e\u043a:\n\nhttps://youtu.be/r-yCbpjVUfw\n\n\u0416\u0435\u043b\u0430\u044e \u0443\u0434\u0430\u0447\u0438\ud83d\udcab", "\u043a\u043e\u043c\u043c\u0435\u043d\u0442\u0430\u0440\u0438\u0439": "\u041f\u043e\u0437\u0434\u0440\u0430\u0432\u043b\u044f\u044e \u0441 \u043f\u0440\u043e\u0445\u043e\u0436\u0434\u0435\u043d\u0438\u0435\u043c \u0435\u0449\u0435 \u043e\u0434\u043d\u043e\u0433\u043e \u0440\u0443\u0431\u0435\u0436\u0430\ud83d\udcaa\ud83d\udd25\n\n\ud83d\udca5 \u0422\u0435\u043f\u0435\u0440\u044c \u0443 \u0442\u0435\u0431\u044f \u043e\u0444\u043e\u0440\u043c\u043b\u0435\u043d\u043d\u044b\u0439 \u043f\u0440\u043e\u0444\u0438\u043b\u044c, \u043f\u043e\u043d\u044f\u0442\u043d\u0430\u044f \u0438\u043d\u0444\u043e\u0440\u043c\u0430\u0446\u0438\u044f \u0432 \u0448\u0430\u043f\u043a\u0435, \u043f\u0440\u0438\u044f\u0442\u043d\u044b\u0435 \u0430\u043a\u0442\u0443\u0430\u043b\u044c\u043d\u044b\u0435 \u0441\u0442\u043e\u0440\u0438\u0441, \u0438 \u043c\u044b \u043c\u043e\u0436\u0435\u043c \u0438\u0434\u0442\u0438 \u0434\u0430\u043b\u044c\u0448\u0435...\n\n\ud83d\udccc 4 \u0443\u0440\u043e\u043a \u043f\u043e\u0441\u0432\u044f\u0449\u0435\u043d \u043a\u043e\u043d\u0442\u0435\u043d\u0442\u0443 - \u043d\u0430\u043f\u043e\u043b\u043d\u0435\u043d\u0438\u044e \u043f\u0440\u043e\u0444\u0438\u043b\u044f.\n\u0421\u0430\u043c\u043e\u0439 \u0438\u043d\u0442\u0435\u0440\u0435\u0441\u043d\u043e\u0435, \u0447\u0442\u043e \u043a\u043e\u043d\u0442\u0435\u043d\u0442 \u0434\u0435\u043b\u0438\u0442\u044c\u0441\u044f \u043d\u0430 \u043d\u0435\u0441\u043a\u043e\u043b\u044c\u043a\u043e \u0432\u0438\u0434\u043e\u0432, \u0438 \u0443 \u043a\u0430\u0436\u0434\u043e\u0433\u043e \u0432\u0438\u0434\u0430 \u0435\u0441\u0442\u044c \u0441\u0432\u043e\u0438 \u0437\u0430\u0434\u0430\u0447\u0438.\n\u041a\u0430\u043a, \u0438 \u0434\u043b\u044f \u0447\u0435\u0433\u043e \u0438\u0441\u043f\u043e\u043b\u044c\u0437\u043e\u0432\u0430\u0442\u044c \u0442\u043e\u0442, \u0438\u043b\u0438 \u0438\u043d\u043e\u0439 \u0432\u0438\u0434 - \u044d\u0442\u043e \u0433\u043b\u0430\u0432\u043d\u0430\u044f \u0442\u0435\u043c\u0430 4 \u0443\u0440\u043e\u043a\u0430.\n\n\u2705 \u041f\u043e\u0435\u0445\u0430\u043b\u0438: \n\nhttps://youtu.be/paUsci1-FtQ\n\n\u041f\u0440\u0438\u044f\u0442\u043d\u043e\u0433\u043e \u0438 \u043f\u043e\u043b\u0435\u0437\u043d\u043e\u0433\u043e \u043f\u0440\u043e\u0441\u043c\u043e\u0442\u0440\u0430\ud83d\udcab", "\u043f\u043e\u0441\u0442": "\u041c\u043e\u043b\u043e\u0434\u0435\u0435\u0435\u0446\ud83d\udc4d\n\u041c\u044b \u043f\u0440\u043e\u0448\u043b\u0438 \u0431\u043e\u043b\u044c\u0448\u0435 \u043f\u043e\u043b\u043e\u0432\u0438\u043d\u044b \u043a\u0443\u0440\u0441\u0430, \u0430 \u0437\u043d\u0430\u0447\u0438\u0442, \u043f\u043e\u0434\u043e\u0448\u043b\u0438 \u043a \u0441\u0430\u043c\u043e\u043c\u0443 \u0438\u043d\u0442\u0435\u0440\u0435\u0441\u043d\u043e\u043c\u0443 \ud83c\udf1f\ud83d\udca5\n\n\u041e\u0431\u044b\u0447\u043d\u043e, \u0431\u043e\u043b\u044c\u0448\u0438\u043d\u0441\u0442\u0432\u043e \u0443\u0447\u0430\u0441\u0442\u043d\u0438\u043a\u043e\u0432 \u0432 \u043e\u0436\u0438\u0434\u0430\u043d\u0438\u0438, \u043a\u043e\u0433\u0434\u0430, \u0443\u0436\u0435, \u043d\u0430\u0447\u043d\u0435\u0442\u0441\u044f \u043f\u0440\u043e \u0440\u0435\u043a\u043b\u0430\u043c\u0443, \u043f\u0440\u043e\u0434\u0432\u0438\u0436\u0435\u043d\u0438\u0435, \u043f\u0440\u0438\u0432\u043b\u0435\u0447\u0435\u043d\u0438\u0435 \u043f\u043e\u0434\u043f\u0438\u0441\u0447\u0438\u043a\u043e\u0432. \n\n\ud83d\udccc \u0422\u0430\u043a, \u0432\u043e\u0442 \u043e\u043d - 5 \u0443\u0440\u043e\u043a \u043f\u0440\u043e \u0431\u0435\u0441\u043f\u043b\u0430\u0442\u043d\u044b\u0435 \u043c\u0435\u0442\u043e\u0434\u044b \u043f\u0440\u043e\u0434\u0432\u0438\u0436\u0435\u043d\u0438\u044f.\n\n\u0414\u0430\u0436\u0435, \u0435\u0441\u043b\u0438 \u0443 \u0442\u0435\u0431\u044f \u0435\u0441\u0442\u044c \u0434\u0435\u043d\u044c\u0433\u0438 \u043d\u0430 \u0440\u0435\u043a\u043b\u0430\u043c\u0443, \u0447\u0442\u043e\u0431 \u043d\u0435 \u0441\u043b\u0438\u0442\u044c \u0438\u0445\ud83d\ude48 , \u043d\u0430\u0434\u043e \u043e\u0431\u044f\u0437\u0430\u0442\u0435\u043b\u044c\u043d\u043e \u0437\u043d\u0430\u0442\u044c \u0438 \u0438\u0441\u043f\u043e\u043b\u044c\u0437\u043e\u0432\u0430\u0442\u044c \u0431\u0435\u0441\u043f\u043b\u0430\u0442\u043d\u044b\u0435 \u043c\u0435\u0442\u043e\u0434\u044b\ud83d\udc4c\n\n\u2705 \u0423\u0440\u043e\u043a \u0442\u0443\u0442 \nhttps://youtu.be/M0PcbUPp0VA - 1 \u0447\u0430\u0441\u0442\u044c\nhttps://youtu.be/kCnpFXiqAx4 -2 \u0447\u0430\u0441\u0442\u044c\n\n\u0423\u0434\u0430\u0447\u0438)", "\u043f\u043e\u043a\u0430\u0437\u0430\u0442\u0435\u043b\u044c": "\u0415\u0445\u0443\u0443\u0443\ud83d\udca5\n\u0415\u0449\u0435 \u043e\u0434\u0438\u043d \u0443\u0440\u043e\u043a \u043f\u0440\u043e\u0439\u0434\u0435\u043d \u0438 \u043c\u044b \u043d\u0430 \u0444\u0438\u043d\u0438\u0448\u043d\u043e\u0439 \u043f\u0440\u044f\u043c\u043e\u0439 \ud83d\ude80 \n\n\ud83d\udccc \u0411\u0435\u0441\u043f\u043b\u0430\u0442\u043d\u044b\u0435 \u043c\u0435\u0442\u043e\u0434\u044b \u0432\u0440\u043e\u0434\u0435 \u0431\u0435\u0441\u043f\u043b\u0430\u0442\u043d\u044b\u0435, \u043d\u043e \u0431\u043e\u043b\u0435\u0435 \u0437\u0430\u0442\u0440\u0430\u0442\u043d\u044b\u0435 \u043f\u043e \u0432\u0440\u0435\u043c\u0435\u043d\u0438 \u0438 \u044d\u043d\u0435\u0440\u0433\u0438\u0438. \u0412 \u043d\u0435\u043a\u043e\u0442\u043e\u0440\u044b\u0445 \u0441\u043b\u0443\u0447\u0430\u044f\u0445, \u0434\u0435\u0439\u0441\u0442\u0432\u0438\u0442\u0435\u043b\u044c\u043d\u043e, \u043b\u0443\u0447\u0448\u0435 \u0437\u0430\u043f\u043b\u0430\u0442\u0438\u0442\u044c \u0434\u0435\u043d\u044c\u0433\u0438 \u0438 \u043f\u0440\u043e\u0434\u0432\u0438\u0433\u0430\u0442\u044c\u0441\u044f \u043f\u043b\u0430\u0442\u043d\u044b\u043c\u0438 \u043c\u0435\u0442\u043e\u0434\u0430\u043c\u0438.\n\n\u2705 \u0412\u0438\u0434\u044b \u043f\u043b\u0430\u0442\u043d\u044b\u0445 \u043c\u0435\u0442\u043e\u0434\u043e\u0432 \u0438 \u043e\u0441\u043e\u0431\u0435\u043d\u043d\u043e\u0441\u0442\u0438 \u043a\u0430\u0436\u0434\u043e\u0433\u043e \u0442\u044b \u0443\u0437\u043d\u0430\u0435\u0448\u044c \u0432 6 \u0443\u0440\u043e\u043a\u0435:\n\nhttps://youtu.be/q1SMia-_bkY\n\n\u0423\u0434\u0430\u0447\u0438\ud83d\udcab", "\u044f\u043c\u043e\u043b\u043e\u0434\u0435\u0446": "\u0422\u044b \u0434\u0435\u0439\u0441\u0442\u0432\u0438\u0442\u0435\u043b\u044c\u043d\u043e \u0437\u0430\u0441\u043b\u0443\u0436\u0438\u0432\u0430\u0435\u0448\u044c \u0443\u0432\u0430\u0436\u0435\u043d\u0438\u044f! \u0422\u044b \u0434\u043e\u0448\u0435\u043b \u0434\u043e \u043a\u043e\u043d\u0446\u0430!\ud83d\udd25\n\n\u0421\u043a\u0430\u0436\u0443 \u043f\u043e \u0441\u0435\u043a\u0440\u0435\u0442\u0443, \u0434\u043e \u044d\u0442\u043e\u0433\u043e \u044d\u0442\u0430\u043f\u0430 \u0434\u043e\u0445\u043e\u0434\u044f\u0442 \u043d\u0435 \u0432\u0441\u0435, \u043f\u043e\u044d\u0442\u043e\u043c\u0443 \u0442\u044b \u043c\u043e\u043b\u043e\u0434\u0435\u0446\ud83d\udc4d\n\n\u0422\u0430\u043a \u0436\u0430\u043b\u044c \u0441 \u0442\u043e\u0431\u043e\u0439 \u043f\u0440\u043e\u0449\u0430\u0442\u044c\u0441\u044f\ud83d\ude15\n\u041f\u043e\u0442\u043e\u043c\u0443 \u0447\u0442\u043e, \u0441\u0435\u0433\u043e\u0434\u043d\u044f, \u044f \u0441\u043a\u0438\u043d\u0443 \u0441\u0441\u044b\u043b\u043a\u0443 \u043d\u0430 \u043f\u043e\u0441\u043b\u0435\u0434\u043d\u0438\u0439 \u0443\u0440\u043e\u043a \u0438 \u043c\u044b \u0441 \u0442\u043e\u0431\u043e\u0439 \u0431\u043e\u043b\u044c\u0448\u0435 \u043d\u0435 \u0443\u0432\u0438\u0434\u0438\u043c\u0441\u044f.\n\u0418\u043b\u0438, \u043c\u043e\u0436\u0435\u0442, \u0410\u0439\u0436\u0430\u043d\u0430 \u043f\u0440\u0438\u0434\u0443\u043c\u0430\u0435\u0442 \u0434\u0440\u0443\u0433\u043e\u0439 \u043a\u0443\u0440\u0441, \u0433\u0434\u0435 \u044f \u0441\u043d\u043e\u0432\u0430 \u0431\u0443\u0434\u0443 \u0434\u0430\u0432\u0430\u0442\u044c \u0443\u0440\u043e\u043a\u0438 \u0438 \u0443\u0432\u0438\u0436\u0443 \u0442\u0435\u0431\u044f\ud83d\ude00\n\n\ud83d\udccc \u041d\u0443 \u0447\u0442\u043e \u0436, \u043f\u043e\u0441\u043b\u0435\u0434\u043d\u0438\u0439, 7 \u0443\u0440\u043e\u043a \u043d\u0443\u0436\u0435\u043d \u0442\u0435\u0431\u044f \u0434\u043b\u044f \u0442\u043e\u0433\u043e, \u0447\u0442\u043e\u0431\u044b \u0438\u0437 \u043f\u043e\u0434\u043f\u0438\u0441\u0447\u0438\u043a\u043e\u0432 \u0434\u0435\u043b\u0430\u0442\u044c \u043f\u043e\u043a\u0443\u043f\u0430\u0442\u0435\u043b\u0435\u0439. \u041c\u043d\u0435, \u043a\u0430\u0436\u0435\u0442\u0441\u044f, \u044d\u0442\u043e \u043e\u0434\u0438\u043d \u0438\u0437 \u0441\u0430\u043c\u044b\u0445 \u0432\u0430\u0436\u043d\u044b\u0445 \u0443\u0440\u043e\u043a\u043e\u0432, \u0445\u043e\u0442\u044c \u0438 \u0